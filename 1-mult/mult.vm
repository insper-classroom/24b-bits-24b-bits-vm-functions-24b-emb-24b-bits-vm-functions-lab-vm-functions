// implemente a funcao mult que recebe dois argumentos
// e retorna a multiplicacao
function mult 0
    push constant 0
    pop temp 2
    label loop
    push argument 1
    push constant 0
    eq
    if-goto end
    push temp 2
    push argument 0
    add
    pop temp 2
    push argument 1
    push constant 1
    sub
    pop argument 1
    goto loop
    label end
    push temp 2
    return